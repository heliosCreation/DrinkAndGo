@{
    ViewData["Title"] = "List";
}

<div class="container-fluid d-flex justify-content-center">

    <div class="row w-75 d-flex flex-column">
        <h1>@Model.Category</h1>
        <div class="row">
            @foreach (Drink drink in Model.Drinks)
            {
                @Html.PartialAsync("_drinkCard", drink).Result;

            }
        </div>

    </div>
</div>

