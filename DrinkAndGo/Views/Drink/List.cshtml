@{
    ViewData["Title"] = "List";
}

<h1>Choose a drink:</h1>
<div class="container-fluid d-flex justify-content-center">
    <div class="row w-75 d-flex">


        @foreach (Drink drink in Model.Drinks)
        {
            @*<partial name="_drinkCard" model="@drink"></partial> Not showing up, strange.*@
            @Html.PartialAsync("_drinkCard", drink).Result;
            

        }

    </div>
</div>

@section scripts{
    <script>
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
}